{
  "executionRoleArn": "arn:aws:iam::557471975685:role/ecsTaskExecutionRole",
  "containerDefinitions": [{
    "name": "iso-server",
    "image": "557471975685.dkr.ecr.ap-northeast-2.amazonaws.com/iso-server",
    "essential": true,
    "portMappings": [{
      "hostPort": 4000,
      "protocol": "tcp",
      "containerPort": 4000
    }],
    "environment": [
      {
        "name": "PORT",
        "value": "4000"
      },
      {
        "name": "NODE_ENV",
        "value": "development"
      },
      {
        "name": "CACHE_ELA_HOST",
        "value": "cache-private-redis-tenant-001.ocp9pr.0001.apn2.cache.amazonaws.com"
      },
      {
        "name": "CACHE_ELA_PORT",
        "value": "6379"
      }
    ]
  }],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048",
  "family": "iso-server"
} 