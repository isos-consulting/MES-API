{
  "executionRoleArn": "arn:aws:iam::557471975685:role/ecsTaskExecutionRole",
  "containerDefinitions": [{
    "name": "iso-server",
    "image": "557471975685.dkr.ecr.ap-northeast-2.amazonaws.com/iso-server",
    "essential": true,
    "portMappings": [{
      "hostPort": 4000,
      "protocol": "tcp",
      "containerPort": 4000
    }],
    "environment": [],
    "environmentFiles": [
      {
        "value": "arn:aws:s3:::s3-isos-environment/test.env",
        "type": "s3"
      }
    ],
    "mountPoints": [],
    "volumesFrom": [],
    "secrets": [],
    "dnsServers": [],
    "dnsSearchDomains": [],
    "extraHosts": [],
    "dockerSecurityOptions": [],
    "dockerLabels": {},
    "ulimits": [],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-create-group": "true",
            "awslogs-group": "/ecs/iso-server",
            "awslogs-region": "ap-northeast-2",
            "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
    },
    "systemControls": []
  }],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2",
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048",
  "family": "iso-server"
} 