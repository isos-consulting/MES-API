  "scripts": {
    "api-docs-all": "yarn api-docs-aut && yarn api-docs-adm && yarn api-docs-std && yarn api-docs-mat && yarn api-docs-sal && yarn api-docs-inv && yarn api-docs-prd && yarn api-docs-out && yarn api-docs-qms",
    "api-docs-adm": "swagger-cli bundle swaggers/sources/adm.swagger.source.yaml --outfile swaggers/builds/adm.swagger.build.yaml --type yaml",
    "api-docs-aut": "swagger-cli bundle swaggers/sources/aut.swagger.source.yaml --outfile swaggers/builds/aut.swagger.build.yaml --type yaml",
    "api-docs-das": "swagger-cli bundle swaggers/sources/das.swagger.source.yaml --outfile swaggers/builds/das.swagger.build.yaml --type yaml",
    "api-docs-std": "swagger-cli bundle swaggers/sources/std.swagger.source.yaml --outfile swaggers/builds/std.swagger.build.yaml --type yaml",
    "api-docs-mat": "swagger-cli bundle swaggers/sources/mat.swagger.source.yaml --outfile swaggers/builds/mat.swagger.build.yaml --type yaml",
    "api-docs-sal": "swagger-cli bundle swaggers/sources/sal.swagger.source.yaml --outfile swaggers/builds/sal.swagger.build.yaml --type yaml",
    "api-docs-inv": "swagger-cli bundle swaggers/sources/inv.swagger.source.yaml --outfile swaggers/builds/inv.swagger.build.yaml --type yaml",
    "api-docs-prd": "swagger-cli bundle swaggers/sources/prd.swagger.source.yaml --outfile swaggers/builds/prd.swagger.build.yaml --type yaml",
    "api-docs-out": "swagger-cli bundle swaggers/sources/out.swagger.source.yaml --outfile swaggers/builds/out.swagger.build.yaml --type yaml",
    "api-docs-qms": "swagger-cli bundle swaggers/sources/qms.swagger.source.yaml --outfile swaggers/builds/qms.swagger.build.yaml --type yaml",
    "refresh-all-cache": "node dist/caches/refresh-all-cache.js",
    "migration-all": "node dist/migrations/create-all-tables.js",
    "migration-undo-all": "node dist/migrations/drop-all-tables.js",
    "cleanup": "rimraf dist",
    "prereset-data": "yarn cleanup && tsc",
    "reset-data": "yarn migration-undo-all && yarn migration-all",
    "prestart": "yarn tsc && node dist/caches/refresh-all-cache.js",
    "start": "tsc-watch --onSuccess \"node --max-old-space-size=4096 dist/bin/www.js\"",
    "pretest": "tsc && yarn reset-data && yarn refresh-all-cache",
    "test": "./node_modules/.bin/jest tests --detectOpenHandles --forceExit --coverage"
  }
